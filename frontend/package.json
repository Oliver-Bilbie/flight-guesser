{
    "name": "flight-guesser-frontend",
    "version": "VERSION_NUMBER",
    "private": true,
    "dependencies": {
        "env-cmd": "^10.1.0",
        "react": "^18.2.0",
        "react-scripts": "^5.0.1",
        "react-dom": "^18.2.0",
        "typescript": "^4.8.5",
        "grommet": "^2.29.1",
        "grommet-icons": "^4.9.0",
        "styled-components": "^5.3.6"
    },
    "devDependencies": {
        "@types/styled-components": "^5.1.2",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "@testing-library/dom": "^8.20.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/react-router": "^5.1.20",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.0",
        "prettier": "^2.8.3",
        "react-test-renderer": "^18.2.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build:prd": "DISABLE_ESLINT_PLUGIN=true env-cmd -f .env.production react-scripts build",
        "build:dev": "DISABLE_ESLINT_PLUGIN=true env-cmd -f .env.development react-scripts build",
        "test": "react-scripts test --watchAll=false",
        "pretty": "prettier --write \"src/**/*.{tsx,ts}\"",
        "lint": "eslint src/components src/helpers --ext .ts,.tsx"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
