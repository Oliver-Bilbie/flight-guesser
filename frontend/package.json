        "test": "react-scripts test --watchAll=false",
        "eject": "react-scripts eject",
        "env": "node -r dotenv/config src/index.tsx",
        "pretty": "prettier --write \"src/**/*.{tsx,ts}\"",
        "lint": "eslint src/*"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
